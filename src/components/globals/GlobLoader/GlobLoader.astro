---
import { Image } from "astro:assets";
import logo from "@assets/kanak-logo.svg";
import recycleIc from "@assets/recycle.svg";
import GlobLoaderMain from './GlobLoader';
import load1 from '@assets/loading-1.png';
import load2 from '@assets/loading-2.png';
import load3 from '@assets/loading-3.png';
import load4 from '@assets/loading-4.png';
import load5 from '@assets/loading-5.png';
import load6 from '@assets/loading-6.png';
import load7 from '@assets/loading-7.png';
import load8 from '@assets/loading-8.png';
import load9 from '@assets/loading-9.png';
import load10 from '@assets/loading-10.png';
import load11 from '@assets/loading-11.png';
import load12 from '@assets/loading-12.png';
---
<GlobLoaderMain client:load>
    <Image src={logo} alt="Kanak Logo" slot="logo" class="img-h"/>
    <Image src={recycleIc} alt="recycle Logo" slot="recycleIc"/>
    <Image src={load1} alt="" slot="load1"/>
    <Image src={load2} alt="" slot="load2"/>
    <Image src={load3} alt="" slot="load3"/>
    <Image src={load4} alt="" slot="load4"/>
    <Image src={load5} alt="" slot="load5"/>
    <Image src={load6} alt="" slot="load6"/>
    <Image src={load7} alt="" slot="load7"/>
    <Image src={load8} alt="" slot="load8"/>
    <Image src={load9} alt="" slot="load9"/>
    <Image src={load10} alt="" slot="load10"/>
    <Image src={load11} alt="" slot="load11"/>
    <Image src={load12} alt="" slot="load12"/>
</GlobLoaderMain>
