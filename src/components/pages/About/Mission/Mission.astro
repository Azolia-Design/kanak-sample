---
import AboutMission from "./Mission";
import { parseLabelsRichtext } from '@utils/text';
const { data } = Astro.props;

---

<AboutMission 
    label={data.mission_label}
    client:visible={{ rootMargin: "200% 0px 200% 0px" }}>
    <Fragment
        set:html={parseLabelsRichtext(Astro.props.data.mission_quote, {
            inputLabel: 'Highlight',
            outputClass: 'txt-green'
        })}
    slot="quote" />
</AboutMission>
