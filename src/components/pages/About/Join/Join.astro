--- 
import AboutJoin from './Join';
import { convertHighlight } from "@utils/text";
const { data } = Astro.props;
---

<AboutJoin 
    label={data.movement_label}
    list={data.movement_list}
    client:visible={{ rootMargin: "200% 0px 200% 0px" }}>
    <Fragment set:html={convertHighlight(data.movement_title)} slot="title" />
</AboutJoin>
