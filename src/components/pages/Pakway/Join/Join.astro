---
import { Image } from "astro:assets";
import PakwayJoin from "./Join";
import joinImg1 from "@assets/pak-join-svg-1.svg";
import joinImg2 from "@assets/pak-join-svg-2.svg";
import joinImg3 from "@assets/pak-join-svg-3.svg";
import joinImg4 from "@assets/pak-join-svg-4.svg";
---

<PakwayJoin
    {...Astro.props}
    client:visible={{ rootMargin: "200% 0px 200% 0px" }}
>
    <Image src={joinImg1} alt="" slot="joinImg1" class="img img-h" />
    <Image src={joinImg2} alt="" slot="joinImg2" class="img img-h" />
    <Image src={joinImg3} alt="" slot="joinImg3" class="img img-h" />
    <Image src={joinImg4} alt="" slot="joinImg4" class="img img-h" />
</PakwayJoin>
