---
import { Image } from "astro:assets";
import PakwayPartner from "./Partner";
import pakImg from "@assets/pak-partner.jpg";
import icArrExt from "@assets/ext-arr-menu.svg?raw";
---

<PakwayPartner
    {...Astro.props}
    client:visible={{ rootMargin: "100% 0px 100% 0px" }}
>
    <Fragment set:html={icArrExt} slot="icArrExt" />
    <Image src={pakImg} alt="" slot="pakImg" class="img-fill" />
</PakwayPartner>
