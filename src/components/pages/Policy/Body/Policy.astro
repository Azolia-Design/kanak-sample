---
import PolicyMain from "./PolicyMain.jsx";
import {generateHtmlFromJson} from '@/js/utils.js';

const titleList = Astro.props.body.filter((item: any) => item.type == "h2");
---
<PolicyMain client:load title_list={titleList} {...Astro.props}>
    <Fragment
        set:html={generateHtmlFromJson(Astro.props.body)}
        slot="content"
    />
</PolicyMain>
