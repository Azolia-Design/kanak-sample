---
import ContactForm from "./Form.jsx";
import { Image } from 'astro:assets';

import icAvatar from "@assets/contact-form-avatar.svg";
import icEmail from "@assets/contact-form-email.svg";
import icSuccess from "@assets/contact-form-succ.svg";
import icArrowUpRight from "@assets/arrow-up-right.svg"
import icArrowDown from "@assets/arrow-down.svg"
---


<ContactForm client:visible>
    <Image src={icAvatar} alt="" class="contact-form-ic active" data-ic="avatar" slot="icAvatar"/>
    <Image src={icEmail} alt="" class="contact-form-ic" data-ic="email" slot="icEmail"/>
    <Image src={icSuccess} alt="" class="contact-form-ic" data-ic="sent" slot="icSuccess"/>
    <Image src={icArrowUpRight} alt="" slot="icArrowUpRight"/>
    <!-- <Image src={icArrowDown} alt="" slot="icArrowDown"/> -->
</ContactForm>