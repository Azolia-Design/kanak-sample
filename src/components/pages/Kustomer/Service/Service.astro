---
import { getByUID } from "@/prismic.js";
import KustomerService from "./Service";

const newList = await Promise.all(
    Astro.props.listItems.map(async (item: any) => {
        let targetItem = await getByUID("service", item);
        return targetItem;
    }),
);
---

<KustomerService
    {...Astro.props}
    list={newList}
    client:visible={{ rootMargin: "100% 0px 100% 0px" }}
/>
