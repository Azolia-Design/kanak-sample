---
import { getAllByType } from "@/prismic.js";

import Image from "astro/components/Image.astro";
import KusomterBenefit from "./Benefit";

const listCaseStudy = await getAllByType("benefit");

const astroUIDs = new Set(Astro.props.list.map((item) => item.uid));
const newList = [];

Astro.props.list.map((el) => {
    listCaseStudy.forEach((item) => {
        item.uid == el && newList.push(item);
    });
});
---

<KusomterBenefit {...Astro.props} itemList={newList} client:visible />
