---
import KustomPackagingHero from "./Hero";
import KustomPackagingHeroThree from "./HeroThree";
import icLoad from "@assets/contact-form-load.svg?raw";
---

<section class="kuspack-hero">
    <div class="container grid">
        <KustomPackagingHero
            {...Astro.props}
            client:visible={{ rootMargin: "200% 0px 200% 0px" }}
        />
        <KustomPackagingHeroThree client:load>
            <Fragment set:html={icLoad} alt="" slot="icLoad"/>
        </KustomPackagingHeroThree>
    </div>
</section>
