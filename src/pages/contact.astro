---
import Layout from "@/layouts/Layout.astro";
import Hero from "@pages/Contact/Hero/Hero.astro";
import Form from "@pages/Contact/Form/Form.astro";

import {getPage} from '@/prismic.js';
const { data } = await getPage('contact');
---

<Layout title={data.page_title} meta_desc={data.meta_desc}>
    <Hero title={data.hero_title[0].text} info_list={data.hero_info}/>
    <Form title={data.form_title} sub_title={data.form_subtitle}/>
</Layout>