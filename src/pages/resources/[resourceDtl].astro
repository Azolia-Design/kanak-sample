---
import { getAllByType } from "@/prismic.js";
import Layout from "@/layouts/Layout.astro";
import Main from "@pages/ResourceDtl/Main/Main.astro";

export async function getStaticPaths() {
    const list = await getAllByType("resource");
    return list.map((item: any) => {
        return {
            params: { resourceDtl: item.uid },
            props: { item },
        };
    });
}
const { resourceDtl } = Astro.params;
const { item } = Astro.props;
---

<Layout title={item.data.title}>
    <Main {...item} />
</Layout>
