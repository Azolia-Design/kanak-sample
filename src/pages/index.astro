---
import Layout from "../layouts/Layout.astro";
import HomeHero from '../components/home/HomeHero.astro';
import HomeProduct from '../components/home/HomeProduct.astro';

import {getPage} from '../prismic.js';
const {data} = await getPage('home');
---

<Layout title="Homepage">
	<HomeHero title={data.hero_title[0].text} sub_title={data.hero_sub_tilte} />
	<HomeProduct title={data.product_title[0].text} product_list={data.product_list}/>

</Layout>