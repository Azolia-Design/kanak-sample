---
import Layout from "@/layouts/Layout.astro";
import Hero from "@pages/Pakway/Hero/Hero.astro";
import Mission from "@pages/Pakway/Mission/Mission.astro";
import Partner from "@pages/Pakway/Partner/Partner.astro";
import Join from "@pages/Pakway/Join/Join.astro";

import { getPage } from "@/prismic.js";
const { data } = await getPage("pakway_page");
---

<Layout
    title={data.page_title}
    meta_desc={data.meta_desc}
    data_page="Pakway"
    footer_cta={data.cta_title[0]?.text}
    footer_bg={data.cta_background}
>
    <Hero data={data}/>
    <Mission data={data} />
    <Partner data={data} />
    <Join data={data}/>
</Layout>
