---
import Layout from "@/layouts/Layout.astro";

import Hero from "@pages/PrivateLabel/Hero/Hero.astro";
import Commitment from "@pages/PrivateLabel/Commitment/Commitment.astro";
import Collection from "@pages/PrivateLabel/Collection/Collection.astro";
import Privilege from "@pages/PrivateLabel/Privilege/Privilege.astro";
import Distribution from "@pages/PrivateLabel/Distribution/Distribution.astro";
import CaseStudies from "@pages/PrivateLabel/CaseStudies/Casestudies.astro";
import Client from "@pages/PrivateLabel/Client/Client.astro";

import { getPage } from "@/prismic.js";

const homeData = await getPage("home");
const { data, uid } = await getPage("privatelabel_page");
---

<Layout
    title="Private Label"
    data_page="Private Label"
    explore_data={uid}
    footer_cta={data.cta_title[0]?.text}
    footer_bg={data.cta_background}
>
    <Hero
        title={data.hero_title}
        smTitle={data.hero_smtitle}
        sub={data.hero_sub}
    />
    <Commitment
        title={data.commitment_title}
        sub={data.commitment_sub}
        btn={data.commitment_btn}
        list={data.commitment_list}
    />
    <Collection
        title={data.collection_title}
        image={data.collection_image}
        btn={data.collection_btn}
        list={data.collection_list}
    />
    <Privilege
        title={data.privilege_title}
        sub={data.privilege_sub}
        list={data.privilege_list}
    />
    <Distribution
        title={data.distribution_title}
        sub={data.distribution_sub}
        btn={data.distribution_btn}
        list={data.distribution_list}
    />
    <CaseStudies
        label={data.kase_studies_label}
        title={data.kase_studies_title}
        des={data.kase_studies_sub}
        btn={data.kase_studies_btn}
    />
    <Client
        title={homeData.data.client_title[0].text}
        sub_title={homeData.data.client_sub_title[0].text}
        client_list={homeData.data.client_list}
    />
</Layout>
